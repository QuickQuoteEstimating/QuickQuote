# Copy this file to `.env` and fill in the values below to configure Supabase.
# These variables are read at build time by Expo. Because they are prefixed with
# `EXPO_PUBLIC_`, they are available in the app via `process.env`.

# The Supabase project URL, e.g. https://xyzcompany.supabase.co
EXPO_PUBLIC_SUPABASE_URL=

# The Supabase project's public anon key
EXPO_PUBLIC_SUPABASE_ANON_KEY=

# (Optional) Redirect URL used by the forgot password flow
EXPO_PUBLIC_SUPABASE_RESET_REDIRECT=
