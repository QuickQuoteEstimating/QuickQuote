{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node16",
    "module": "Node16",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "types": ["react", "react-native", "expo"]
  },
  "include": [
    "app",
    "src",
    "index.tsx",
    "expo-env.d.ts"
  ]
}
